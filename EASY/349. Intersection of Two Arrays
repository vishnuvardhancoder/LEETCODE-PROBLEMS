#PYTHON

class Solution:
    def intersection(self, nums1: List[int], nums2: List[int]) -> List[int]:
        res =[]
        for i in nums1: 
            for j in nums2:
                if(i == j):
                    res.append(i)
                    break

        final = []
        for i in res:
            if i not in final:
                final.append(i)
        return final
        
